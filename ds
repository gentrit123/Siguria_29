import sys
import os
from os import sys
import xml.etree.ElementTree as xml
import base64

m=sys.argv[1]
def func():
 #Dekriptimi me caesar crypt ne gjuhen programuese python
 def dekriptimi(tekst,qelesi):
     rezultati = ""
     #Kalimi neper secilin karakter
     for i in range(len(teksti)):
         char = teksti[i]
         
         if(char.isupper()):
             rezultati += chr((ord(char) - int(qelesi) - 65) % 26 +65)
             
             
         elif(char.islower()):
            rezultati += chr((ord(char) - int(qelesi) - 97) % 26 + 97)
            
         elif(char<chr(65)):
             rezultati += char
             
         else:
             rezultati += " "
             
     return rezultati
